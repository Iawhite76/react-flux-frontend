article.context
  font-size 16px
  margin-top 35px
  
  
 #react_search 
	text-align center
 	
	ul
		padding 0
 
	input[type=text]
		font inherit
		border 6px solid #a3d8d6
		padding 5px 20px
		margin-bottom 20px
		font-size 15px
		box-shadow 0 1px 1px #DDD
		width 100%
		outline none
		display block
		color #7B8585
		margin 0 auto
	

	ul
	  list-style none
	  display inline-block
	  width 100%
	  text-align left
	

	ul li
	  display block
	  padding 15px 20px
	  background-color #F8F8F8
	  color #7B8585
	  margin-bottom 3px
	  position relative
	  
	  transition 0.3s
	

	ul li a
	  position absolute
	  left 160px
	  font-size 12px
	  line-height 16px
	  color #969d9d
	

	ul lihover
	  background-color #d8f2f1
	
