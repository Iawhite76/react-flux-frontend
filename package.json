{
  "name": "another-flux-app",
  "version": "0.0.0",
  "description": "A React/Flux app generate by RF, powered with Babel",
  "main": "build/bundle.js",
  "dependencies": {
    "flux": "^2.0.3",
    "immutable": "^3.7.4",
    "lodash": "^3.10.0",
    "object-assign": "^2.1.1",
    "react": "^0.13.3",
    "rsvp": "^3.0.18",
    "superagent": "^1.2.0",
    "wordpress-rest-api": "^0.4.0"
  },
  "devDependencies": {
    "babel-jest": "*",
    "babel-loader": "*",
    "bootstrap": "*",
    "bootstrap-webpack": "*",
    "css-loader": "*",
    "file-loader": "*",
    "fs": "0.0.2",
    "imports-loader": "*",
    "jest-cli": "*",
    "jquery": "*",
    "react-hot-loader": "*",
    "react-tools": "*",
    "style-loader": "*",
    "stylus": "*",
    "stylus-loader": "*",
    "url-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server --colors --content-base ./build",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "globals": {
      "SCRIPTS": "<rootDir>/src/scripts"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
